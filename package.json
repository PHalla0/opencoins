{
  "name": "@opencoins/launchpad-plugin",
  "version": "1.0.0",
  "description": "Token launchpad service for OpenCode.ai - Deploy EVM and Solana tokens with AI assistance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE",
    "CONFIGURATION.md",
    "docs/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "compile-contracts": "cd src/evm/contracts && solc --abi --bin --optimize Token.sol -o ../../../build/contracts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "opencode",
    "opencode-plugin",
    "opencode-ai",
    "plugin",
    "token",
    "launchpad",
    "evm",
    "ethereum",
    "solana",
    "blockchain",
    "defi",
    "crypto",
    "web3",
    "smart-contracts"
  ],
  "author": "OpenCoins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/singulargh/opencoins.git"
  },
  "bugs": {
    "url": "https://github.com/singulargh/opencoins/issues"
  },
  "homepage": "https://github.com/singulargh/opencoins#readme",
  "dependencies": {
    "@opencode-ai/plugin": "latest",
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.9",
    "ethers": "^6.13.0",
    "dotenv": "^16.4.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.6.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
